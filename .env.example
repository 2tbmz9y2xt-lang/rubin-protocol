# RUBIN node â€” dev environment template
# Copy to .env and fill in values before running locally.
# For production/systemd see: operational/systemd/rubin-node.env.example

# wolfCrypt shim (required for PQ signatures in non-dev builds)
RUBIN_WOLFCRYPT_SHIM_PATH=
RUBIN_WOLFCRYPT_SHIM_SHA3_256=
RUBIN_WOLFCRYPT_STRICT=1

# Node config
RUBIN_DATADIR=~/.rubin/data
RUBIN_CHAIN=devnet

# Conformance runner (optional overrides)
RUBIN_CONFORMANCE_CMD_TIMEOUT_S=900
